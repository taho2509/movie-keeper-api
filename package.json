{
  "name": "movie-keeper-api",
  "version": "1.0.0",
  "description": "Base REST api web server built using Koa framework, typescript, clean arquitecture and some good practices",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "npx tsc -p tsconfig.prod.json",
    "start": "node -r dotenv/config dist/index.js",
    "prestart": "npm run build",
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon -r dotenv/config --exec ts-node src/index.ts",
    "start:test": "ts-node src/index.ts",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "coverage": "jest --coverage",
    "newman": "newman run postman_collection/movie-keeper-api.postman_collection.json --bail",
    "stubby": "node node_modules/stubby/bin/stubby -d stubs/dependencies.yaml -s 4000 -t 8084 -a 8893",
    "functional-test": "ts-node functional/index.ts"
  },
  "author": "Angel Ulice Tabares Gonzalez <taho2509@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "axios": "^0.23.0",
    "cls-hooked": "^4.2.2",
    "correlation-id": "^4.0.0",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "mongoose": "^6.0.10",
    "node-nats-streaming": "^0.3.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@shopify/jest-koa-mocks": "^3.0.5",
    "@types/cls-hooked": "^4.3.3",
    "@types/istanbul-lib-report": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.0.3",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa-router": "^7.4.4",
    "@types/mongodb": "^4.0.7",
    "@types/newman": "^5.1.4",
    "@types/npm": "^7.19.0",
    "@types/stubby": "^4.1.1",
    "@types/uuid": "^8.3.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "mongodb-memory-server": "^7.4.4",
    "newman": "^5.3.0",
    "nodemon": "^2.0.13",
    "npm": "^7.24.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stubby": "^5.1.0",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  }
}
